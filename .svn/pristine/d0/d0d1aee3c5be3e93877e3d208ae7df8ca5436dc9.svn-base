body {
	background: #f4f9fa;
	font-family: 'Open Sans', sans-serif;
	margin: 0 auto;
}

/* HEADER(LOGO&PESQUISAR) */
header.header{
	background: #FFF;
	width: 100%;
	height: 50px;
	line-height: 50px;
}
/* FIM HEADER */

/* NAVBAR MENU */
nav.navtop{
	background: #4dafe2;
	color: #FFF;
	width: 100%;
	height: 31px;
	line-height: 32px;
	box-shadow: 0px 3px 0px 0px #2687d3;	
}

nav.navtop div{
	color: #FFF;
	margin-left: 20%;
	margin-right: 20%;
}

ul.none{
	list-style-type: none;	
    margin: 0;
    padding: 0;    
}

ul.none li{
	float: left;
	display: inline;
}

ul.none li a{
	color: #FFF;
	text-decoration: none;
	padding: 6px 15px;
	margin: 0;
}

ul.none li a:hover:not(.active) {
    background-color: #399bda;
}

.active {
    background-color: #2687d3;
}

/* FIM NAVBAR MENU */

/* CONTEUDO */

div.content{
	padding: 10px;
	background: #FFF;
	width: 80%;
	min-height: 400px;
	margin: 1% auto;
	box-shadow: 0px 3px 3px rgba(0,0,0,0.11);
}

/* FIM CONTEUDO */